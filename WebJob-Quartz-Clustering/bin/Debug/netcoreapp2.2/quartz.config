quartz.scheduler.instanceName = LocalServer
quartz.scheduler.instanceId = AUTO
quartz.serializer.type = binary
quartz.threadPool.threadCount = 1
quartz.jobStore.type = Quartz.Impl.AdoJobStore.JobStoreTX, Quartz
quartz.jobStore.useProperties = true
quartz.jobStore.dataSource = MeuBanco
quartz.jobStore.clusterCheckinInterval = 600
quartz.jobStore.clustered = true
quartz.jobStore.lockHandler.type = Quartz.Impl.AdoJobStore.UpdateLockRowSemaphore, Quartz
quartz.dataSource.MeuBanco.connectionString = Server=(localdb)\MSSQLLocalDB;Database=configuracoes_quartz;Trusted_Connection=true;
quartz.dataSource.MeuBanco.provider = SqlServer